<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE edf>
<edf version="1.0">
    <PDE Name="AdvReact">
        <Name>Advection-Reaction</Name>
        <Equation>
            <Parameter Widget="Label">
                <Name> Options </Name>
            </Parameter>
            <Parameter Widget="Combo">
                <Name>Convection</Name>
                <Type> String </Type>
                <Item>
                    <Name>None</Name>
                </Item>
                <Item>
                    <Name>Computed</Name>
                </Item>
                <Item>
                    <Name>Constant</Name>
                </Item>
            </Parameter>
            <Parameter Widget="Label">
                <Name>Free text input</Name>
            </Parameter>
            <Parameter Widget="TextEdit" Enabled="True">
                <Name> Free text </Name>
                <Type> String </Type>
